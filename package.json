{
  "name": "itrabble",
  "version": "1.1.1",
  "description": "Library to add useful methods to JavaScript iterables eg. Array, Map, Set, String, TypedArray",
  "main": "dist/index.js",
  "module": "index.js",
  "engines": {
    "node": ">=6.11.1"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
